{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_final_orders",
						"type": "DatasetReference"
					},
					"name": "dforderstest"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_customers_test",
						"type": "DatasetReference"
					},
					"name": "orderitemsink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          ORDER_ID as short,",
				"          LINE_ITEM_ID as short,",
				"          PRODUCT_ID as short,",
				"          UNIT_PRICE as double,",
				"          QUANTITY as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> dforderstest",
				"dforderstest sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> orderitemsink"
			]
		}
	}
}